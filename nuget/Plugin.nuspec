<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8.1">
    <id>Plugin.AppShortcuts</id>
    <version>0.1.0-alpha</version>
    <title>AppShortcuts Plugin for Xamarin and Windows</title>
    <authors>Aden Earnshaw</authors>
    <owners>Aden Earnshaw</owners>
    <licenseUrl>https://github.com/adenearnshaw/AppShortcutsPlugin/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/adenearnshaw/AppShortcutsPlugin</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/adenearnshaw/AppShortcutsPlugin/master/art/appshortcuts_icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Dynamically add and remove shortcuts to your app's icon so the user can deep-link into your app.
    </description>
    <summary>Manage App Shortcuts for Xamarin and Windows</summary>
    <tags>xamarin, pcl, xam.pcl, shortcuts, deeplink, jumplist, uwp, android, xamarin.forms, ios</tags>
    <dependencies>
       <group targetFramework="net">
      </group> 
       <group targetFramework="win">
      </group> 
      <group targetFramework="netstandard1.0">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="MonoAndroid10">
      </group>
      <group targetFramework="Xamarin.iOS10">
      </group>
      <!-- <group targetFramework="Xamarin.Mac20">
      </group> -->
      <!-- <group targetFramework="portable-net45+win+wpa81+wp80">
      </group> -->
       <group targetFramework="uap">
      </group> 
      <group targetFramework="dotnet">
      </group>
      <!-- <group targetFramework="xamarintvos">
      </group> -->
      <!-- <group targetFramework="xamarinwatchos">
      </group> -->
    </dependencies>
  </metadata>
  <files>
    <!-- NetStandard -->
    <file src="..\src\Plugin.AppShortcuts\bin\Release\netstandard1.0\Plugin.AppShortcuts*.dll" target="lib\netstandard1.0" />

    <!-- Xamarin.Android -->
    <file src="..\src\Plugin.AppShortcuts.Android\bin\Release\Plugin.AppShortcuts*.dll" target="lib\MonoAndroid10" />

    <!-- Xamarin.iOS -->
    <file src="..\src\Plugin.AppShortcuts.iOS\bin\Release\Plugin.AppShortcuts*.dll" target="lib\Xamarin.iOS10" />

    <!-- UWP -->
    <file src="..\src\Plugin.AppShortcuts.UWP\bin\Release\Plugin.AppShortcuts*.dll" target="lib\UAP10"/>
  </files>
</package>